@model List<Products>


<div class="modal-body">
    <h4>Your Favorite Products</h4>
    @if (Model.Count == 0)
    {
        <p>You have no favorite products yet.</p>
    }
    else
    {
        <ul>
            @foreach (var product in Model)
            {
                <li>@product.Title</li> 
            }
        </ul>
    }
</div>

@model List<Products>

	
<div class="container">

	<div class="table">
		<div class="table-header">
			<div class="header__item"><a id="name" class="filter__link" href="#">Title</a></div>
			<div class="header__item"><a id="wins" class="filter__link filter__link--number" href="#">Price</a></div>
			<div class="header__item"><a id="draws" class="filter__link filter__link--number" href="#">Brand</a></div>
			<div class="header__item"><a id="losses" class="filter__link filter__link--number" href="#">Category</a></div>
			
			
		</div>
		@foreach (var products in Model)
		{
			<div class="table-content">
				<div class="table-row">
					<div class="table-data">@products.Title</div>
					<div class="table-data">@products.Price</div>
					<div class="table-data">@products.brand.Name</div>
					<div class="table-data">@products.category.Name</div>
				</div>
			</div>
		}
	</div>
</div>